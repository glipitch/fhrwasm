          "Display": "5 - Very Good",
          "Position": 0
        },
        "4": {
          "Display": "4 - Good",
          "Position": 1
        },
        "3": {
          "Display": "3 - Generally Satisfactory",
          "Position": 2
        },
        "2": {
          "Display": "2 - Improvement Necessary",
          "Position": 3
        },
        "1": {
          "Display": "1 - Major Improvement Necessary",
          "Position": 4
        },
        "0": {
          "Display": "0 - Urgent Improvement Necessary",
          "Position": 5
        },
        "Exempt": {
          "Position": 8
        },
        "AwaitingPublication": {
          "Position": 6,
