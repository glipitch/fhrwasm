{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BaseUrl": "https://api.ratings.food.gov.uk/",
  "DistributionTemplates": {
    "FHIS": {
      //Scotland
      "SchemeName": "Food Hygiene Information System",

      "Ratings": {
        "Pass and Eat Safe": {
          "Position": 0
        },
        "Improvement Required": {
          "Position": 2
        },
        "Awaiting Inspection": {
          "Position": 3
        },
        "Pass": {
          "Position": 1
        },
        "Exempt": {
          "Position": 4
        }
      }
    },
    "FHRS": {
      "SchemeName": "Food Hygiene Rating Scheme",

      "Ratings": {
        "AwaitingInspection": {
          "Display": "Awaiting Inspection",
          "Position": 7
        },
